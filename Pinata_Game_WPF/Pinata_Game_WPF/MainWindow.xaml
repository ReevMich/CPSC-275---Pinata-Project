<Window x:Class="Pinata_Game_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pinata_Game_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Canvas Name="TheCanvas">
        <Grid Name="TheGrid">
            <Line Name="myLine" X1="350" Y1="20" X2="350" Y2="300" StrokeThickness="2" Stroke="Black">
            </Line>
        </Grid>
        <Label x:Name="lbl_gameOver" Content="Game Over" Canvas.Left="115" Canvas.Top="48" FontWeight="Bold" FontSize="64" FontFamily="Impact" />
    </Canvas>
</Window>